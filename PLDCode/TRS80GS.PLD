/* set uart to 0 if IN or OUT and address >= 0xe0 and <= 0xe7 */
field uart_inputs = [dev_in, dev_out, a_seven, a_six, a_five, a_four, a_three];
field uart_outputs = [uart];

TABLE uart_inputs => uart_outputs {
['b'0000000..'b'0011111]=>'b'1;
['b'0100000..'b'0111011]=>'b'1;
'b'0111100=>'b'0;
['b'0111101..'b'0111111]=>'b'1;
['b'1000000..'b'1011011]=>'b'1;
'b'1011100=>'b'0;
['b'1011101..'b'1111111]=>'b'1;
}
